<protocol>
    <name>UW Prostate</name>
    <ct>
        <protocol institution="Johnson Creek">>8.1 XRT PELVIS</protocol>
        <protocol institution="East">8.2 Pelvis without contrast 0.8sec</protocol>
        <protocol institution="UWHC">XRT_PELVIS</protocol>
    </ct>
    <institutions>
        <institution>UWHC</institution>
        <institution>East</institution>
        <institution>Johnson Creek</institution>
    </institutions>
    <diagnoses>
        <icd>C61</icd>
    </diagnoses>
    <order>
        <name>Prostate Only</name>
        <prefix>Pros_</prefix>
        <prescription>
            <roi>
                <name>PTV</name>
                <type>DX</type>
                <volume units="%">95</volume>
                <dose units="Gy">70</dose>
            </roi>
            <fractions>28</fractions>
            <frequency>Daily</frequency>
            <imaging>3D Volumetric</imaging>
            <modality>VMAT</modality>
        </prescription>
        <goals>
            <roi>
                <name>Rectum</name>
                <type dir="lt">VX</type>
                <volume units="%">15</volume>
                <dose units="Gy">60</dose>
                <priority>2</priority>
            </roi>
            <roi>
                <name>Rectum</name>
                <type dir="lt">VX</type>
                <volume units="%">35</volume>
                <dose units="Gy">40</dose>
                <priority>2</priority>
            </roi>
            <roi>
                <name>Bladder</name>
                <type dir="lt">VX</type>
                <volume units="%">20</volume>
                <dose units="Gy">50</dose>
                <priority>2</priority>
            </roi>
        </goals>
    </order>
    <order>
        <name>Prostate with Nodes</name>
        <prefix>Pros_</prefix>
        <prescription>
            <roi>
                <name>PTV</name>
                <type>DX</type>
                <volume units="%">95</volume>
                <dose units="Gy">70</dose>
            </roi>
            <fractions>28</fractions>
            <frequency>Daily</frequency>
            <imaging>3D Volumetric</imaging>
            <modality>VMAT</modality>
        </prescription>
        <goals>
            <roi>
                <name>Rectum</name>
                <type dir="lt">VX</type>
                <volume units="%">15</volume>
                <dose units="Gy">60</dose>
                <priority>2</priority>
            </roi>
            <roi>
                <name>Rectum</name>
                <type dir="lt">VX</type>
                <volume units="%">35</volume>
                <dose units="Gy">40</dose>
                <priority>2</priority>
            </roi>
            <roi>
                <name>Bladder</name>
                <type dir="lt">VX</type>
                <volume units="%">50</volume>
                <dose units="Gy">50</dose>
                <priority>2</priority>
            </roi>
        </goals>
    </order>
    <order>
        <name>Prostate Sequential</name>
        <prefix>Pros_</prefix>
        <prescription>
            <roi>
                <name>PTV</name>
                <type>DX</type>
                <volume units="%">95</volume>
                <dose units="Gy">50.4</dose>
            </roi>
            <fractions>28</fractions>
            <frequency>Daily</frequency>
            <imaging>3D Volumetric</imaging>
            <modality>VMAT</modality>
        </prescription>
        <prescription>
            <roi>
                <name>PTV_Prostate</name>
                <type>DX</type>
                <volume units="%">95</volume>
                <dose units="Gy">28.8</dose>
            </roi>
            <fractions>16</fractions>
            <frequency>Daily</frequency>
            <imaging>3D Volumetric</imaging>
            <imaging>2D Planar</imaging>
            <modality>VMAT</modality>
            <modality>3DCRT</modality>
        </prescription>
        <goals>
            <roi>
                <name>Rectum</name>
                <type dir="lt">VX</type>
                <volume units="%">15</volume>
                <dose units="Gy">60</dose>
                <priority>2</priority>
            </roi>
            <roi>
                <name>Rectum</name>
                <type dir="lt">VX</type>
                <volume units="%">35</volume>
                <dose units="Gy">40</dose>
                <priority>2</priority>
            </roi>
            <roi>
                <name>Bladder</name>
                <type dir="lt">VX</type>
                <volume units="%">50</volume>
                <dose units="Gy">50</dose>
                <priority>2</priority>
            </roi>
        </goals>
    </order>
    <goals>
        <roi>
            <name>PTV</name>
            <type dir="ge">VX</type>
            <volume units="%">95</volume>
            <dose units="%" roi="PTV">99</dose>
            <priority>1</priority>
        </roi>
        <roi>
            <name>PTV</name>
            <type>Max</type>
            <dose units="%" roi="PTV">110</dose>
            <priority>1</priority>
        </roi>
        <roi>
            <name>PenileBulb</name>
            <type dir="lt">VX</type>
            <volume units="%">50</volume>
            <dose units="Gy">50</dose>
            <priority>2</priority>
        </roi>
        <roi>
            <name>Bowel</name>
            <type dir="lt">VX</type>
            <volume units="cc">175</volume>
            <dose units="Gy">45</dose>
            <priority>2</priority>
        </roi>
        <roi>
            <name>FemoralHeadR</name>
            <type>Max</type>
            <dose units="Gy">40</dose>
            <priority>2</priority>
        </roi>
        <roi>
            <name>FemoralHeadL</name>
            <type>Max</type>
            <dose units="Gy">40</dose>
            <priority>2</priority>
        </roi>
        <roi>
            <name>Urethra</name>
            <type>Max</type>
            <dose units="%" roi="PTV">100%</dose>
            <priority>2</priority>
        </roi>
        <roi>
            <name>Skin</name>
            <type dir="lt">VX</type>
            <volume units="cc">1</volume>
            <dose units="Gy">30</dose>
            <priority>1</priority>
        </roi>
        <goalset>
            <name>QUANTEC</name>
            <priority>4</priority>
        </goalset>
    </goals>
    <planning>
        <roi>
            <name>External</name>
            <type>External</type>
            <threshold>-500</threshold>
        </roi>
        <roi>
            <name>Skin</name>
            <type>Derived</type>
            <start>External</start>
            <inner units="cm">0.5</inner>
        </roi>
        <roi>
            <name>Ring</name>
            <type>Derived</type>
            <start>PTV</start>
            <expand units="cm">1</expand>
            <subtract>PTV</subtract>
        </roi>
        <beam>
            <name>Arc_1_CW</name>
            <energy>6</energy>
            <gantry type="start">181</gantry>
            <gantry type="end">179</gantry>
            <collimator>5</collimator>
        </beam>
        <beam>
            <name>Arc_2_CCW</name>
            <energy>6</energy>
            <gantry type="start">179</gantry>
            <gantry type="end">181</gantry>
            <collimator>355</collimator>
        </beam>
    </planning>
    <optimization>
        <name>Default Optimization</name>
        <phase>
            <description>Initial optimization</description>
            <dosegrid>
                <x units="cm">0.2</x>
                <y units="cm">0.2</y>
                <z units="cm">0.2</z>
            </dosegrid>
            <constraint>
                <roi>PTV</roi>
                <type>Min</type>
                <dose units="%" roi="PTV">100</dose>
                <weight>10</weight>
            </constraint>
            <constraint>
                <roi>PTV</roi>
                <type>Uniform</type>
                <dose units="%" roi="PTV">100</dose>
                <weight>10</weight>
            </constraint>
            <constraint>
                <roi>PTV</roi>
                <type>Max</type>
                <dose units="%" roi="PTV">100</dose>
                <weight>10</weight>
            </constraint>
            <constraint>
                <roi>Rectum</roi>
                <type>DVH</type>
                <dose units="Gy">30</dose>
                <volume units="%">30</volume>
                <weight>1</weight>
            </constraint>
            <constraint>
                <roi>Bladder</roi>
                <type>DVH</type>
                <dose units="Gy">30</dose>
                <volume units="%">30</volume>
                <weight>1</weight>
            </constraint>
        </phase>
        <phase>
            <description>1st of 2 warm starts</description>
            <iterations>50</iterations>
        </phase>
        <phase>
            <description>2nd of 2 warm starts</description>
            <constraint>
                <roi>PTV</roi>
                <type>Uniform</type>
                <dose units="%" roi="PTV">100</dose>
                <weight>20</weight>
            </constraint>
            <iterations>50</iterations>
        </phase>
        <phase>
            <description>Reduce OAR dose</description>
            <reduceOAR>10</reduceOAR>
        </phase>
        <phase>
            <description>Normalize Dose</description>
            <normalize>
                <dose units="%" roi="PTV">100</dose>
                <volume units="%">95</volume>
            </normalize>
        </phase>
    </optimization>
</protocol>